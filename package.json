{
  "name": "startupshop",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.4.1",
  "scripts": {
    "dev": "pnpm --filter @startupshop/web dev",
    "build": "pnpm -r build",
    "typecheck": "pnpm -r typecheck",
    "validate:listings": "pnpm --filter @startupshop/core validate:listings",
    "test:api": "pnpm --filter @startupshop/web test:api",
    "db:migrate": "pnpm --filter @startupshop/web prisma:migrate",
    "db:generate": "pnpm --filter @startupshop/web prisma:generate"
  }
}
